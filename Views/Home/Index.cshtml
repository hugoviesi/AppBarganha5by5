@{
}

@model AppBarganhaWEB.Models.HomeVO

<h1>Bem-Vindo, @Model.UsuarioLogado.Nome</h1>

<div class="col-md-10">
    @Html.ActionLink("Anunciar", "Index", "Anuncio")
</div>

<div class="col-md-10">
    @Html.ActionLink("Sair", "LogOut", "Login")
</div>

<div class="col-md-10">
    @Html.ActionLink("Leilão", "Index", "Leilao")
</div>

<h2>Seus Anuncios</h2>

@foreach(var anuncio in Model.Anuncios)
{
<div>
    <p> Nome: @anuncio.Nome </p>
    <p> Descricao: @anuncio.Descricao </p>
    <p> Valor: @anuncio.Valor </p>
</div>         
}